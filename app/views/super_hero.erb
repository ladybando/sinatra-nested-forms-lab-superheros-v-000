<form action='/super_hero' method="GET">
  <h1>Create a Team and Heroes!</h1>
  <h2>Team Ruby</h2>
  <label for="team[name]">Team Name:</label><br>
  <input id="team_name" type="text" name="team[name]" /><br>
  <label for="team[motto]">Team Motto:</label><br>
  <input id="team_motto" type="text" name="team[motto]" /><br>

<h2>Hero 1</h2>
<p><label for="member1_name">Ship Name:</label>
  <input type="text" name="heroes[teams][][name]" id="member1_name"></p>
<p><label for="member1_power">Ship Type:</label>
  <input type="text" name="heroes[teams][][power]" id="member1_power"></p>
<p><label for="member1_bio">Ship Booty:</label>
  <input type="text" name="hero[teams][][biography]" id="member1_bio"></p>

<h2>Hero 2</h2>
<p><label for="member2_name">Hero Name:</label>
  <input type="text" name="heroes[teams][][name]" id="member2_name"></p>
<p><label for="member2_power">Hero Power:</label>
  <input type="text" name="heroes[teams][][power]" id="member2_power"></p>
<p><label for="member2_bio">Hero Biography:</label>
  <input type="text" name="heroes[teams][][biography]" id="member2_bio"></p>

  <h2>Hero 3</h2>
<p><label for="member3_name">Hero Name:</label>
  <input type="text" name="heroes[teams][][name]" id="member3_name"></p>
<p><label for="member3_power">Hero Power:</label>
  <input type="text" name="heroes[teams][][power]" id="member3_power"></p>
<p><label for="member3_bio">Hero Biography:</label>
  <input type="text" name="heroes[teams][][biography]" id="member3_bio"></p>
  <input id="submit" type="submit">
</form>
